{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Download Dataset",
            "type": "debugpy",
            "request": "launch",
            "program": "download_dataset.py",
            "console": "integratedTerminal",
            "args": [
                "halfcheetah-expert-v2"
            ]
        },
        {
            "name": "Generate Dataset",
            "type": "debugpy",
            "request": "launch",
            "program": "generate_dataset.py",
            "console": "integratedTerminal",
            "args": [
                "-t",
                "reach",
                "-e",
                "panda-reach-dense",
                "-n",
                "1000",
                "-o",
                "datasets/panda-reach-dense.pkl"
            ]
        },
        {
            "name": "Train Single",
            "type": "debugpy",
            "request": "launch",
            "program": "train_single.py",
            "console": "integratedTerminal",
            "args": [
                "-c", "configs/tqcher_panda_push_dense.yaml",
                "--training_kwargs", "{'n_timesteps': 1000}'"
            ]
        },
        ,
        {
            "name": "Evaluate Single",
            "type": "debugpy",
            "request": "launch",
            "program": "evaluate_single.py",
            "console": "integratedTerminal",
            "args": [
                "-c", "configs/tqcher_panda_push_dense.yaml",
                "-l", "results/tqcher-panda-reach-dense.zip",
                "--render"
            ]
        },
    ]
}